{
  "name": "pogo/symfony-queue",
  "type": "symfony-bundle",
  "description": "FrankenPHP Queue Driver for Symfony Messenger",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "symfony/config": "^6.4 || ^7.0",
    "symfony/dependency-injection": "^6.4 || ^7.0",
    "symfony/http-kernel": "^6.4 || ^7.0",
    "symfony/messenger": "^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Pogo\\Queue\\Symfony\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pogo\\Queue\\Symfony\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "conflict": {
    "symfony/framework-bundle": "<6.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
